/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/* 暂无数据 */
.noData {
  margin: 30rpx;
  text-align: center;
}

.logo {
  margin-top: 127rpx;
  margin-bottom: 90rpx;
  text-align: center;
  font-size: 50rpx;
  color: #378dbb;
}

.login .item {
  padding: 10rpx 50rpx;
}

.login .item .input {
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 20rpx;
  border: 2rpx solid #378dbb;
  text-align: center;
}

.login .item .btn {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 20rpx;
  border: 2rpx solid #378dbb;
  background-color: #378dbb;
  color: #fff;
}


.fs {
  font-size: 40rpx;
}