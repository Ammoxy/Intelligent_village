/* pages/personal/device/device.wxss *//* 列表 */
.list {
  /* padding: 0 0 40rpx 40rpx; */
  padding: 0 10rpx 0 10rpx;
  background-color: #fff;
}

.tableviewcell {
  position: relative;
  border-bottom: 2rpx solid #dcdcdc;
  color: #666;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 35rpx;
}

.tableviewcell .adress {
  width: 90%;
  font-size: 28rpx;
  margin-left: 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.tableviewcell .right-icon {
  width: 40rpx;
  height: 40rpx;
  margin: 0 0 0 10rpx;
}

.iden-btn {
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  margin-top: 10rpx;

  /* padding: 25rpx 25rpx 25rpx 0; */

}