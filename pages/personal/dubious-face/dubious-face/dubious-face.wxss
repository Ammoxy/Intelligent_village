page {
  height: 100%;
  width: 100%;
  font-size: 28rpx;
}
.box {
  background-color: #fff;
}

.box .top {
  width: 100%;
  height: 65rpx;
}

.box .header {
  text-align: center;
  position: fixed;
  width: 100%;
  display: flex;
  z-index: 100;
  height: 65rpx;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
}

.box .header .header_img {
  flex: 1;
  padding: 10rpx;
}

.box .header .header_box {
  flex: 1;
  padding: 10rpx;

}

.box .header .header_btn {
  flex: 1;
  padding: 10rpx;
}

.box .list {
  text-align: center;
  width: 100%;
  /* height: 120rpx; */
  border-bottom: 2rpx solid #ddd;
  background-color: #fff;
  color: #666;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.box .list .info-list {
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: space-between;
}
.box .list:nth-child(2n + 1) {
  background-color: #fff;
}

.box .list:nth-child(2n) {
  background-color: #f0f5f8;
}

.box .list .info-list .list_img {
  align-items: center;
  flex: 1;
}

.box .list .info-list .list_box {
  align-items: center;
  flex: 1;
  padding: 10rpx;
}

.box .list .info-list .list_btn {
  align-items: center;
  flex: 1;
  padding: 10rpx;
}

.box .list .info-list .list_img image {
  width: 68rpx;
  height: 90rpx;
  padding: 10rpx;
}

.box .list .info-list .guardian {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.box .list text {
  width: 170rpx;
}

.box .list .relationship {
  width: 200rpx;
}

.list .info-list .list_btn .btn {
  width: 156rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin: 0;
  color: #fff;
  background-color: #378dbb;
  border-radius: 10rpx;
  font-size: 30rpx;
}

button::after {
  border: none;
}
